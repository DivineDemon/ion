generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String  @id @default(cuid())
  imageUrl   String?
  email      String  @unique
  firstName  String?
  lastName   String?
  grantId    String?
  grantEmail String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
